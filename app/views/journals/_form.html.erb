<%= form_for @journal do |f| %>
    <%= f.hidden_field :horoscope_id %>
    <%= f.collection_select :horoscope_id, Horoscope.all, :id, :ascendant, prompt: true %><br><br>

    <%= f.label :title %><br>
    <%= f.text_field :title %><br><br>
    <%= f.label :description %><br>
    <%= f.text_area :description %><br><br>
    <%= f.label :mood %><br>
    <%= f.text_field :mood %><br><br>

    <%= f.submit %>
<% end %>  